<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Circle?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="275.0" prefWidth="300.0">
         <children>
            <Button layoutX="134.0" layoutY="2.0" mnemonicParsing="false" onAction="#up" text="Up" />
            <Button layoutX="253.0" layoutY="126.0" mnemonicParsing="false" onAction="#right" text="Right" />
            <Button layoutX="125.0" layoutY="249.0" mnemonicParsing="false" onAction="#down" text="Down" />
            <Button layoutY="126.0" mnemonicParsing="false" onAction="#left" text="Left" />
            <Circle fx:id="myCircle" fill="#94ffd8" layoutX="150.0" layoutY="138.0" radius="40.0" stroke="BLACK" strokeType="INSIDE" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
