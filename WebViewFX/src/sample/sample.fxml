<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.WebView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
       <WebView fx:id="webView" layoutY="65.0" prefHeight="435.0" prefWidth="700.0" />
       <TextField fx:id="textField" layoutY="15.0" onAction="#loadPage" prefHeight="23.0" prefWidth="700.0" />
       <Button layoutY="40.0" mnemonicParsing="false" onAction="#loadPage" text="load" />
       <Button layoutX="41.0" layoutY="40.0" mnemonicParsing="false" onAction="#refreshPage" text="refresh" />
       <Button layoutX="96.0" layoutY="40.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
       <Button layoutX="121.0" layoutY="40.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
       <Button layoutX="142.0" layoutY="40.0" mnemonicParsing="false" onAction="#displayHistory" text="history" />
       <Button layoutX="196.0" layoutY="40.0" mnemonicParsing="false" onAction="#backPage" text="back" />
       <Button layoutX="238.0" layoutY="40.0" mnemonicParsing="false" onAction="#forwardPage" text="forward" />
       <Button layoutX="298.0" layoutY="40.0" mnemonicParsing="false" onAction="#executeJS" text="execute JS" />
   </children>
</AnchorPane>
