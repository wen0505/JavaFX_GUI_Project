<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label fx:id="myLabel" alignment="CENTER" layoutY="50.0" prefHeight="29.0" prefWidth="300.0" text="Welcome!" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button fx:id="myButton" layoutX="122.0" layoutY="270.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <TextField fx:id="myTextField" layoutX="44.0" layoutY="180.0">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Label layoutX="95.0" layoutY="150.0" text="Enter your age">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
